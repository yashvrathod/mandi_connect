{
  "expo": {
    "name": "Mandi Connect",
    "slug": "mandi",
    "version": "1.0.0",
    "scheme": "mandiconnect",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "icon": "./assets/images/Buyer.png",
    "splash": {
      "image": "./assets/images/background.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#1E7D3A"
    },
    "updates": {
      "enabled": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "eas": {
        "projectId": "e748ff2d-a564-4912-8cb6-a37c0ec6ab90"
      },
      "router": {}
    },
    "android": {
      "package": "com.mandiconnect.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Buyer.png",
        "backgroundColor": "#10B981"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ],
    "owner": "yashsdsdsdsds-organization",
    "ios": {
      "bundleIdentifier": "com.mandiconnect.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    }
  }
}
